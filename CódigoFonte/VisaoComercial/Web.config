    <!-- CONFIGURACAO EMAIL -->
    <add key="servidorSMTP" value="smtp-web-sp.kinghost.net"/>
    <add key="emailContato" value="notificacao@sustentar.inf.br" />
    <!-- CONFIGURACAO SITE -->
    <add key="idConfig" value="0" />
    <!-- CONFIGURACAO SEO -->
    <add key="TituloAplicacao" value="Sistema Ambiental" />
    <add key="url" value="http://www.c2ti.com.br" />
    <add key="language" value="Portuguese" />
    <add key="description" value="C2TI" />
    <add key="keywords" value="c2ti" />
    <add key="reply-to" value="contato@c2ti.com.br" />
